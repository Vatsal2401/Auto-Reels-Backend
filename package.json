{
  "name": "ai-reels-backend",
  "version": "0.0.1",
  "description": "AI-generated faceless reels platform backend",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d src/database/data-source.ts",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/database/data-source.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/database/data-source.ts",
    "setup": "bash scripts/setup.sh",
    "dev": "bash scripts/dev.sh",
    "seed": "ts-node scripts/seed.ts",
    "copy-music-to-s3": "ts-node -r dotenv/config scripts/copy-music-supabase-to-s3.ts",
    "upload-showcase-clips": "ts-node -r dotenv/config scripts/upload-showcase-clips.ts",
    "seed-showcase": "ts-node -r dotenv/config scripts/seed-showcase-items.ts",
    "update-showcase": "ts-node -r dotenv/config scripts/update-showcase.ts",
    "db:reset": "npm run migration:revert && npm run migration:run && npm run seed",
    "infra:up": "cd docker && docker-compose -f docker-compose.dev.yml up -d postgres && cd ..",
    "infra:down": "cd docker && docker-compose -f docker-compose.dev.yml down && cd ..",
    "infra:logs": "cd docker && docker-compose -f docker-compose.dev.yml logs -f postgres && cd ..",
    "infra:tools": "cd docker && docker-compose -f docker-compose.dev.yml --profile tools up -d && cd .."
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/lib-storage": "^3.980.0",
    "@aws-sdk/s3-request-presigner": "^3.490.0",
    "@elevenlabs/elevenlabs-js": "^2.33.0",
    "@google-cloud/text-to-speech": "^6.4.0",
    "@google/genai": "^1.38.0",
    "@google/generative-ai": "^0.24.1",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/serve-static": "^4.0.1",
    "@nestjs/swagger": "^7.1.17",
    "@nestjs/throttler": "^6.5.0",
    "@nestjs/typeorm": "^10.0.1",
    "@react-email/components": "^1.0.7",
    "@react-email/render": "^2.0.4",
    "axios": "^1.13.5",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.67.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "class-validator-jsonschema": "^5.1.0",
    "compression": "^1.8.1",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "fluent-ffmpeg": "^2.1.2",
    "glob": "^13.0.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.9.2",
    "nodemailer": "^7.0.13",
    "openai": "^4.20.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-microsoft": "^1.0.0",
    "pg": "^8.11.3",
    "razorpay": "^2.9.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "reflect-metadata": "^0.1.13",
    "replicate": "^1.4.0",
    "resend": "^6.9.1",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.16",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.3.0",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.8.1",
    "@types/express": "^4.17.25",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.11",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.0.0",
    "@types/nodemailer": "^7.0.9",
    "@types/passport-google-oauth20": "^2.0.14",
    "@types/passport-jwt": "^4.0.1",
    "@types/react": "^19.2.11",
    "@types/react-dom": "^19.2.3",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
